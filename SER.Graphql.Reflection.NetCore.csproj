<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net5.0</TargetFramework>
		<Company>SER Soluciones</Company>
		<Version>0.13.3</Version>
		<AssemblyName>SER.Graphql.Reflection.NetCore</AssemblyName>
		<RootNamespace>SER.Graphql.Reflection.NetCore</RootNamespace>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Description>This is a complement to graphql-dotnet (https://github.com/graphql-dotnet/graphql-dotnet) to avoid boilerplate
</Description>
	</PropertyGroup>
	<ItemGroup>

		<PackageReference Include="GraphQL" Version="3.1.3" />
		<PackageReference Include="GraphQL.SystemTextJson" Version="3.1.3" />
		<PackageReference Include="GraphQL.Authorization" Version="3.1.0" />
		<PackageReference Include="GraphQL.Conventions" Version="2.5.5" />
		<PackageReference Include="GraphQL.Server.Transports.AspNetCore.SystemTextJson" Version="4.3.0" />
		<PackageReference Include="GraphQL.Server.Ui.Playground" Version="4.3.0" />

		<PackageReference Include="GraphQL-Parser" Version="5.3.3" />
		<PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL.NetTopologySuite" Version="5.0.0-rc1" />

		<PackageReference Include="System.Linq.Dynamic.Core" Version="1.2.5" />
		<PackageReference Include="NetTopologySuite.IO.GeoJSON" Version="2.0.4" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.DynamicLinq" Version="3.2.5" />

		
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="Repositories\" />
	</ItemGroup>
</Project>
